<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.medicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MedicApp">
        <activity android:name=".UpdateActivity" android:parentActivityName=".DoctorsActivity"
            android:label="Update Doctor"></activity>
        <activity
            android:name=".AddDoctor"
            android:label="Add Doctor"
            android:parentActivityName=".DoctorsActivity" />
        <activity
            android:name=".DoctorsActivity"
            android:label="Doctors List" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>